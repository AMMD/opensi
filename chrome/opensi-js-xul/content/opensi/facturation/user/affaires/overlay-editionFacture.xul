												<menulist id="oef-modeEnvoiFacture" oncommand="oef_setModifie(true);">
							            <menupopup>
														<menuitem label="Courrier" value="C" />
														<menuitem label="Fax" value="F" />
														<menuitem label="Mail" value="M" />
							            </menupopup>
							        	</menulist>
							        </hbox>
						        </row>
					      	</rows>
					     	</grid>
					     	
					     	<grid>
					       	<columns>
					        	<column />
					         	<column />
					       	</columns>
