<overlay
  id="opensi_facturation_user_affaires_overlayEditionFacture"
	xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <script src="overlay-editionFacture.js" />

	<vbox id="editionFacture" flex="1">
	
		<deck id="oef-deckFacture" flex="1">

		  <vbox flex="1" class="auto-overflow">
				<hbox>
					<tabbox flex="1" pack="center" id="oef-tabBoxFacture">
						<tabs>
							<tab label="Facture" id="oef-tabFacture" />
							<tab label="Versions" />
						</tabs>
						<tabpanels flex="1">
			    		<tabpanel pack="center" align="start" orient="horizontal">
					      <grid>
					       	<columns>
					        	<column />
					         	<column />
					       	</columns>
					       	<rows>
					       		<row align="center">
											<label class="label" value="N° Affaire :" />
											<label id="oef-numAffaire" />
					         	</row>
					         	<row align="center">
					          	<label class="label" value="N° Facture :" />
					           	<label id="oef-numFacture" />
					         	</row>
										<row align="center">
						          <label class="label" value="Date facture :" />
											<label id="oef-dateFacture" />
					          </row>
					          <row align="center">
					          	<label value="Pôle d'activité :" class="label" />
					          	<hbox>
					        			<menulist id="oef-secteur" ref="http://www.opensi.org/facturation/secteurs_activite" datasources="rdf:null" style="width:180px" oncommand="oef_setModifie(true);">
													<menupopup>
														<menuitem label="[Aucun]" value="0" />
													</menupopup>
													<template>
					          				<menupopup>
															<menuitem uri="rdf:*" label="rdf:http://www.opensi.org/facturation/secteurs_activite/rdf#Libelle" value="rdf:http://www.opensi.org/facturation/secteurs_activite/rdf#Secteur_Id" />
					          				</menupopup>
													</template>
					        			</menulist>
					          	</hbox>
					          </row>
					          <row align="center">
											<label class="label" value="Mode d'envoi de la facture :" />
