						  			  	<menulist id="oef-codeTarif" ref="http://www.opensi.org/facturation/liste_types_tarifs" datasources="rdf:null" oncommand="oef_setModifie(true);">
					 			  				<template>
					             			<menupopup>
															<menuitem uri="rdf:*" label="rdf:http://www.opensi.org/facturation/liste_types_tarifs/rdf#Libelle" value="rdf:http://www.opensi.org/facturation/liste_types_tarifs/rdf#Valeur" />
					             			</menupopup>
													</template>
						  				  </menulist>
						  				</hbox>
					  				</row>
					         	<row align="center">
					            <label value="" />
