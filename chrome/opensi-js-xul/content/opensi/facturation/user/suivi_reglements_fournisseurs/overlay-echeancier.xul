											<menupopup>
												<menuitem label="[Choisir]" value="0" />
											</menupopup>
											<template>
												<menupopup>
													<menuitem uri="rdf:*" label="rdf:http://www.opensi.org/facturation/fournisseurs/rdf#Libelle" value="rdf:http://www.opensi.org/facturation/fournisseurs/rdf#Fournisseur_Id" />
